// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Poll {
  id         String   @id @default(uuid())
  question   String
  status     String
  start_date DateTime
  end_date   DateTime

  Option Option[]
  @@map("polls")
}

